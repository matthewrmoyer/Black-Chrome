{
	"manifest_version": 2,
	"name": "Black Chrome Extension",
	"description": "Changes website backgrounds to black",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": [
	"activeTab"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent":false
	}

}